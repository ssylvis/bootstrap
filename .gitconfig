[user]
	email = sean@cardspring.com
	name = Sean Sylvis
[alias]
	a = add
	aa = add .
	ap = add --patch
	alias = !git config -l | grep alias
	b = branch
	bd = branch -d
	bdd = branch -D
	bm = branch -m
	c = commit
	ce = commit --allow-empty
	cm = commit -m
	co = checkout
	cob = checkout -b
	com = checkout master
	cpush = commit --amend
	cpusha = "!git add .; git commit --amend --no-edit"
	cpushe = commit --amend --no-edit
	diffg = !git diff HEAD^ | tig
	diffh = diff HEAD^
	diffs = diff HEAD^ --stat=100,80
	logd = !git log --format='%h %C(yellow)(%an) %Creset%s (%cr)'
	logg = "!f() { git log --format='%h %C(yellow)(%an)%C(cyan)%d%Creset %s (%cr)' --grep=$1 --regexp-ignore-case --extended-regexp; }; f"
	logh = !git log --format='%h %C(yellow)(%an)%C(cyan)%d%Creset %s (%cr)' -n 20 | cut -c -140
	prune = remote prune origin
	pullo = pull origin
	pushf = push -f origin
	pushm = push origin master
	pusho = push origin
	r = rebase
	ra = rebase --abort
	rc = rebase --continue
	ri = rebase -i
	rim = rebase -i master
	rspop = reset --hard HEAD^
	rsrm = reset --hard
	rsundo = reset HEAD^
	s = status
	sa = stash apply
	sd = stash drop
	sl = stash list
	sp = stash pop
	st = stash
[core]
	editor = vi
